{
  "name": "@igniter-js/telemetry",
  "version": "0.1.12",
  "description": "Type-safe telemetry library for Igniter.js with session-based events, typed event registry, transport adapters, and sampling/redaction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "src/shim.ts",
    "dist/shim.js",
    "dist/shim.mjs"
  ],
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "AGENTS.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.cjs"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.mjs",
      "require": "./dist/adapters/*.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "igniter",
    "igniterjs",
    "telemetry",
    "observability",
    "logging",
    "tracing",
    "events",
    "session",
    "sampling",
    "redaction",
    "typescript",
    "type-safe"
  ],
  "author": "Felipe Barcelos <felipebarcelospro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/felipebarcelospro/igniter-js.git",
    "directory": "packages/telemetry"
  },
  "bugs": {
    "url": "https://github.com/felipebarcelospro/igniter-js/issues"
  },
  "homepage": "https://igniterjs.com",
  "devDependencies": {
    "@igniter-js/core": "*",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "^2.1.9",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@igniter-js/core": ">=0.2.0",
    "@igniter-js/store": ">=0.1.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
